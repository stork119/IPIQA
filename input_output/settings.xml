<?xml version="1.0"?>
<main>
    <CONFIG>
        <parameter key="number_of_cores" value= "4"/>
        <parameter key="sample_number" value="3"/>
        <parameter key="job_done" value="TRUE"/>
    </CONFIG>
    <TASK class="TASK_QUEUE">   
        <parameters_by_value/>
        <parameters_by_name/>
        <TASK class="TASK_PARALLELIZE">
            <parameters_by_value>
                <parameter key="sleep_time"  value="5"/>
                <parameter key="pathway_package_path" value = "C://Users//Setnea//Desktop//"/>
                <parameter key="input_path.2"  value="input//"/>
                <parameter key="output_path.2" value="output//"/>
                <parameter key="folders_number" value="96"/> 
                <parameter key="quantify_data_raw_path.2" value="raw//"/>
                <parameter key="pathway_data_path.2"  value="raw//"/>
            </parameters_by_value>
            <parameters_by_name>
		<parameter key="input_path.1" value = "pathway_package_path"/>
		<parameter key="output_path.1" value = "pathway_package_path"/>
            </parameters_by_name>
            <TASK class="TASK_QUEUE">   
                <parameters_by_value/>
                <parameters_by_name/>
                <TASK class="TASK_DOWNLOAD">
                    <parameters_by_value>
                        <parameter key="by_url" value="FALSE"/>
                    </parameters_by_value>
                    <parameters_by_name>
                        <parameter key="input_path.2"  value="folder_name"/>
                        <parameter key="input_path.1"  value="input_path"/>
                    </parameters_by_name>
                    <update_by_value>
                    </update_by_value>
                    <update_by_name>
                        <update key="pathway_data_path" value = "output_path"/>
                    </update_by_name>
                </TASK>
                <update_by_value/>
                <update_by_name/>
            </TASK>
            <update_by_value/>
            <update_by_name>
                <update key="quantify_data_raw_path" value="quantify_data_raw_path"/>
                <update key="pathway_data_path" value = "pathway_data_path"/>
            </update_by_name>
        </TASK>
        <update_by_value/>
        <update_by_name/>
    </TASK>
</main>