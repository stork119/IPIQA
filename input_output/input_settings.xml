    <task class="MERGE">
        <parameters_by_value>
            <parameter key="input_path" value="example_value">
            </parameter>
            <parameter key="output_path" value="example_value">
            </parameter>
            <parameter key="folders_number" value="example_value">
            </parameter>
        </parameters_by_value>
        <parameters_by_name>
            <parameter key="input_path" value="example_value_name">
            </parameter>
            <parameter key="output_path" value="example_value_name">
            </parameter>
            <parameter key="folders_number" value="example_name">
            </parameter>
        </parameters_by_name>
        <update_by_value>
        </update_by_value>
        <update_by_name>
        </update_by_name>
    </task>
</main>