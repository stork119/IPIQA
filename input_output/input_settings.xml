                </parameters_by_name>
                <update_by_value>
                </update_by_value>
                <update_by_name>
                </update_by_name>
            </task>
            <task class="CELL_PROFILER">
                <parameters_by_value>
                    <parameter key="input" value="example_value">
                    </parameter>
                    <parameter key="output" value="example_value">
                    </parameter>
                    <parameter key="pipeline" value="example_value">
                    </parameter>
                </parameters_by_value>
                <parameters_by_name>
                    <parameter key="input" value="example_value_name">
                    </parameter>
                    <parameter key="output" value="example_value_name">
                    </parameter>
                    <parameter key="pipeline" value="example_value_name">
